{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/searchList/index",
      "style": {
        "navigationBarTitleText": "搜索列表"
      }
    },
    {
      "path": "pages/detail/index",
      "style": {
        "navigationBarTitleText": "民宿详情"
      }
    },
    {
      "path": "pages/order/index",
      "style": {
        "navigationBarTitleText": "订单"
      }
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/setting/index",
      "style": {
        "navigationBarTitleText": "设置"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/regist/index",
      "style": {
        "navigationBarTitleText": "注册"
      }
    }
  ],
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#224141",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/home/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_selected.png"
      },
      {
        "text": "订单",
        "pagePath": "pages/order/index",
        "iconPath": "static/tabbar/order.png",
        "selectedIconPath": "static/tabbar/order_selected.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/index",
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my_selected.png"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^My(.*)": "@/components/My$1/index.vue"
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#224141",
    "backgroundColor": "#F5f5f5"
  }
}
